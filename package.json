  ],
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "npm install bower nodemon -g && bower install",
    "build-css": "node-sass --include-path scss scss/_base.scss public/stylesheets/main.css && node-sass --include-path scss scss/_baseadmin.scss public/stylesheets/mainadmin.css ",
    "watch-css": "nodemon -e scss -x \"npm run build-css && nodemon\"",
    "watch": "start & npm run watch-css"
  },
  "dependencies": {
    "angular-utils-pagination": "^0.11.1",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.0",
    "body-parser": "~1.15.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "csurf": "^1.9.0",
    "debug": "~2.2.0",
    "do-wrapper": "^3.11.1",
    "express": "~4.14.0",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.14.2",
    "express-validator": "^3.1.2",
    "hbs": "~4.0.1",
    "moment": "^2.18.1",
    "mongoose": "^4.7.5",
    "mongoose-paginate": "^5.0.3",
    "morgan": "~1.10.0",
    "node-sass": "^4.5.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "serve-favicon": "~2.3.0",
    "uuid": "^3.0.1"
  }
}
